<div class="firstPartPermissions">
    <div class="form-check form-switch">
        <input disabled {% if branche.permission.isReadResa() %}checked {% endif%} class="form-check-input" type="checkbox" role="switch" >
        <label class="form-check-label" >Lire les réservations</label>
    </div>
    <div class="form-check form-switch">
        <input disabled {% if branche.permission.isEditResa() %}checked {% endif%} class="form-check-input" type="checkbox" role="switch" >
        <label class="form-check-label" >Modifier les réservations</label>
    </div>
    <div class="form-check form-switch">
        <input disabled {% if branche.permission.isRemoveResa() %}checked {% endif%} class="form-check-input" type="checkbox" role="switch" >
        <label class="form-check-label" >Supprimer les réservations</label>
    </div>
    <div class="form-check form-switch">
        <input disabled {% if branche.permission.isReadPayment() %}checked {% endif%} class="form-check-input" type="checkbox" role="switch" >
        <label class="form-check-label" >Lire les paiements</label>
